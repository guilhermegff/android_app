<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_tela_final"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/tela1"
    tools:context="com.example.guilherme.avaliacaoalimentar.TelaFinal">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="90dp"
        android:id="@+id/image4"
        android:src="@drawable/tela2"

        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text8"
        android:layout_below="@id/image4"
        android:layout_centerHorizontal="true"
        android:text="Obrigado pela sua avaliação"

        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text9"
        android:layout_below="@id/text8"
        android:layout_centerHorizontal="true"
        android:text="Estamos trabalhando para melhorar ainda mais a sua alimentação escolar."

        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text10"
        android:layout_below="@id/text9"
        android:layout_centerHorizontal="true"
        android:text="Continue avaliando todos os dias, você só tem a ganhar."

        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text11"
        android:layout_below="@id/text10"
        android:layout_centerHorizontal="true"
        android:text="Continue avaliando todos os dias, você só tem a ganhar."

        />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/botaoConcluir"
        android:layout_below="@id/text11"
        android:layout_centerHorizontal="true"
        android:src="@drawable/resp1"
        android:onClick="pegaResp"

        />
</RelativeLayout>
