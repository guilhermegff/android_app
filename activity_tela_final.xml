<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_tela_final"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/tela1"
    tools:context="com.example.guilherme.avaliacaoalimentar.TelaFinal">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="130dp"
        android:id="@+id/image4"
        android:src="@drawable/telafinal1"

        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text8"
        android:layout_below="@id/image4"
        android:layout_marginTop="150dp"
        android:layout_centerHorizontal="true"
        android:text="@string/telaFinalText1"
        android:textAlignment="center"
        style="@style/styleTelaFinal"

        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text9"
        android:layout_below="@id/text8"
        android:layout_centerHorizontal="true"
        android:text="@string/telaFinalText2"
        android:textAlignment="center"
        style="@style/styleTelaFinal"


        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text10"
        android:layout_below="@id/text9"
        android:layout_centerHorizontal="true"
        android:textAlignment="center"
        android:text="@string/telaFinalText3"
        style="@style/styleTelaFinal"

        />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/botaoConcluir"
        android:layout_marginTop="30dp"
        android:layout_below="@id/text10"
        android:layout_centerHorizontal="true"
        android:src="@drawable/telafinal2"
        android:onClick="botaoConcluir"

        />
</RelativeLayout>
